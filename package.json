{
  "name": "base-dev-env",
  "version": "0.1.0",
  "description": "Base ES6, React, Express, Node development environment.",
  "scripts": {
    "clean:dist": "rimraf ./dist && mkdir dist",
    "build:dev:server": "babel src/server/devServer.js --out-file dist/server.js",
    "build:prod:server": "babel src/server/prodServer.js --out-file dist/server.js",
    "build:dev:web": "webpack --config webpack.config.dev.babel.js",
    "build:prod:web": "webpack --config webpack.config.prod.babel.js",
    "lint": "babel-node node_modules/eslint/bin/eslint.js -- src/**",
    "prestart:dev": "npm-run-all lint clean:dist build:dev:server build:dev:web",
    "server": "node dist/server.js",
    "watch": "npm run build:dev:web -- --progress --colors --watch",
    "start:dev": "npm-run-all --parallel server watch",
    "start": "npm-run-all clean:dist build:prod:server build:prod:web server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "js"
  ],
  "author": "Ray Vernagus",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.15.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "morgan": "^1.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-latest": "^6.24.1",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-cli": "^1.1.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "babel-preset-react": "^6.24.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "uglify-js": "^2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^2.4.1"
  }
}
